#include "planning_class.h"
#include <ros/ros.h>
#include <geometry_msgs/Twist.h>


void temp_class::setVel(const double x, const double y) {
    vel.linear.x = x;
    vel.linear.y = y;
    pub.publish(vel);
    ROS_INFO_STREAM("publishing velocity command: " << "linear (x)=" << vel.linear.x << ", angular (z)=" << vel.linear.y);  
}
